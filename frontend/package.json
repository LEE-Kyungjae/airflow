{
  "name": "crawler-review-ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src/ --max-warnings 0",
    "test:e2e": "playwright test --config=../tests/e2e/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=../tests/e2e/playwright.config.ts --ui",
    "test:e2e:report": "playwright show-report ../tests/e2e/playwright-report",
    "test:e2e:headed": "playwright test --config=../tests/e2e/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=../tests/e2e/playwright.config.ts --debug",
    "test:e2e:chromium": "playwright test --config=../tests/e2e/playwright.config.ts --project=chromium",
    "test:e2e:firefox": "playwright test --config=../tests/e2e/playwright.config.ts --project=firefox",
    "test:e2e:webkit": "playwright test --config=../tests/e2e/playwright.config.ts --project=webkit"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.17.0",
    "autoprefixer": "^10.4.24",
    "axios": "^1.13.5",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.18.0",
    "vite": "^7.3.1",
    "vitest": "^2.1.8"
  }
