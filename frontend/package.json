  "dependencies": {
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.17.0",
    "autoprefixer": "^10.4.24",
    "axios": "^1.6.7",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
